<Application
    x:Class="MotionsRace.WindowsPhone.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:conv="using:MotionsRace.WindowsPhone.Converters"
    xmlns:local="using:MotionsRace.WindowsPhone">
    <Application.Resources>
        <conv:VisibilityValueConverter x:Key="VisibilityValueConverter"/>
        <conv:InvertedVisibilityValueConverter x:Key="InvertedVisibilityValueConverter"/>
        <conv:NativeColorValueConverter x:Key="NativeColorValueConverter"/>
        <conv:CategoryImageConverter x:Key="CategoryImageConverter"/>
        <conv:DesignTimeConverter x:Key="DesignTimeConverter"/>        
    </Application.Resources>
</Application>